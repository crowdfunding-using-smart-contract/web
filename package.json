{
	"name": "@fund-o/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint --ignore-path .eslintignore ./ --ext .ts,.tsx --max-warnings 10",
		"lint:fix": "eslint --ignore-path .eslintignore --fix ./ --ext .ts,.tsx",
		"pretty": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|tsx|json)\"",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"pnpm run lint:fix",
			"pnpm run pretty"
		]
	},
