		"dayjs": "^1.11.10",
		"embla-carousel-react": "^7.1.0",
		"ethers": "^6.11.1",
		"formik": "^2.4.5",
		"framer-motion": "^11.0.23",
		"humps": "^2.0.1",
		"i18next": "^23.10.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^14.0.5",
