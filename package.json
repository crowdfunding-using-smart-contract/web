{
	"name": "@fund-o/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint --ignore-path .eslintignore ./ --ext .ts,.tsx --max-warnings 10",
		"lint:fix": "eslint --ignore-path .eslintignore --fix ./ --ext .ts,.tsx",
		"pretty": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|tsx|json)\"",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx}": [
			"pnpm run lint:fix",
			"pnpm run format"
		]
	},
	"dependencies": {
		"@heroicons/react": "^2.1.1",
		"@mantine/core": "^7.5.2",
		"@mantine/dates": "^7.5.2",
		"@mantine/hooks": "^7.5.2",
		"@tanstack/react-query": "^5.15.3",
		"axios": "^1.6.3",
		"dayjs": "^1.11.10",
		"formik": "^2.4.5",
		"humps": "^2.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.0.1",
		"react-router": "^6.21.1",
		"react-router-dom": "^6.21.1",
		"yup": "^1.3.3",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@types/humps": "^2.0.6",
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^8.0.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.2.2",
		"vite": "^5.0.8"
	}
}
